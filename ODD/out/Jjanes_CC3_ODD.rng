               <s:report xmlns="http://www.tei-c.org/ns/1.0"
                         xmlns:s="http://purl.oclc.org/dsdl/schematron"
                         test="@from and @n">n et from ne peuvent pas
                                    coexister</s:report>
            </rule>
         </pattern>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="odd_eugene_roma-biblScope-fromto-constraint-report-8">
            <rule context="tei:biblScope">
               <s:report xmlns="http://www.tei-c.org/ns/1.0"
